# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_service_role_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Setup Key (para criar super admin via API)
SETUP_KEY=premium-car-setup-2024

# Neon PostgreSQL Database Configuration
# Recommended for most uses (with connection pooling via pgbouncer)
DATABASE_URL=postgresql://user:password@host-pooler.region.aws.neon.tech/dbname?sslmode=require

# For uses requiring a connection without pgbouncer
DATABASE_URL_UNPOOLED=postgresql://user:password@host.region.aws.neon.tech/dbname?sslmode=require

# PostgreSQL Connection Parameters
PGHOST=host-pooler.region.aws.neon.tech
PGHOST_UNPOOLED=host.region.aws.neon.tech
PGUSER=your_database_user
PGDATABASE=your_database_name
PGPASSWORD=your_database_password

# Vercel Postgres Templates
POSTGRES_URL=postgresql://user:password@host-pooler.region.aws.neon.tech/dbname?sslmode=require
POSTGRES_URL_NON_POOLING=postgresql://user:password@host.region.aws.neon.tech/dbname?sslmode=require
POSTGRES_USER=your_database_user
POSTGRES_HOST=host-pooler.region.aws.neon.tech
POSTGRES_PASSWORD=your_database_password
POSTGRES_DATABASE=your_database_name
POSTGRES_URL_NO_SSL=postgresql://user:password@host-pooler.region.aws.neon.tech/dbname
POSTGRES_PRISMA_URL=postgresql://user:password@host-pooler.region.aws.neon.tech/dbname?connect_timeout=15&sslmode=require

# Neon Auth environment variables for Next.js
NEXT_PUBLIC_STACK_PROJECT_ID=your_stack_project_id_here
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=your_stack_publishable_client_key_here
STACK_SECRET_SERVER_KEY=your_stack_secret_server_key_here
